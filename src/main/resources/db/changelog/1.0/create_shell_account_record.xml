<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd"
                   objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">

    <changeSet id="1" author="Jinhan He">
        <createTable tableName="shell_account_record">
            <column name="id" type="bigint" remarks="主键">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="shell_account_id" type="bigint" remarks="贝克账户ID">
                <constraints nullable="false"/>
            </column>
            <column name="quantity" type="int" defaultValue="0" remarks="数量">
                <constraints nullable="false"/>
            </column>
            <column name="type" type="varchar(10)" remarks="变动类型">
                <constraints nullable="false"/>
            </column>
            <column name="source" type="varchar(10)" remarks="来源">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
